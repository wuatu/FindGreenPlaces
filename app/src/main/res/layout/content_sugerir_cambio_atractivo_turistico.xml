<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:orientation="vertical"
    tools:context=".SugerirCambioAtractivoTuristico"
    tools:showIn="@layout/activity_sugerir_cambio_atractivo_turistico">

    <!-- TODO: Update blank fragment layout -->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageViewVAT"
                    android:layout_width="wrap_content"
                    android:layout_height="200dp"
                    android:layout_weight="1"
                    android:src="@drawable/addimagen" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10sp">

                <TextView
                    android:id="@+id/textViewTituloAT2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5sp"
                    android:text="Titulo"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/divider2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:background="@color/com_facebook_button_background_color_disabled" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="10sp">

                    <!--Descripcion-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewDescripcion"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Descripción"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="18sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewDescripcionAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Descripción"
                            android:textColor="@color/com_facebook_button_background_color_focused_disabled"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarDescripcion"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarDescripcion"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Descripción"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin descripcion-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />
                    <!--Tips de viaje-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewTips"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Tips de Viaje"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="18sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewTipsAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="tips"
                            android:textColor="@color/com_facebook_button_background_color_focused_disabled"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarTips"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarTips"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Tips de Viaje"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin tips de viaje-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5sp"
                        android:text="Información Adicional"
                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <!--Horario de atencion-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewHorario"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Horario de Atención"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewHorarioAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="tips"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarHorario"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarHorario"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Horario de Atención"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin horario de atencion-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />
                    <!--Telefono-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewTelefono"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Teléfono"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewTelefonoAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="telefono"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarTelefono"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarTelefono"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Teléfono"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin telefono-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />
                    <!--Página web-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewPagina"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Página Web"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewPaginaAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="pagina web"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarPagina"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarPagina"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Página Web"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin página web-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />
                    <!--Redes sociales-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textViewRedes"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Redes Sociales"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/textViewRedesAT"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="redes sociales"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLatyoutModificarRedes"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewModificarRedes"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:text="Modificar Redes Sociales"
                                android:textColor="@color/colorAccent" />
                        </LinearLayout>
                    </LinearLayout>
                    <!--fin redes sciales-->
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20px"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Categorias"
                            android:textColor="@color/common_google_signin_btn_text_dark_focused"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/linearLatyoutCategorias"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <com.cunoraz.tagview.TagView xmlns:tagview="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/tag_group2"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp"
                                android:gravity="center_horizontal"
                                tagview:lineMargin="5dp"
                                tagview:tagMargin="5dp"
                                tagview:textPaddingBottom="5dp"
                                tagview:textPaddingLeft="8dp"
                                tagview:textPaddingRight="8dp"
                                tagview:textPaddingTop="5dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLatyoutmodificarCategorias"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textViewModificarCategorias"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="right"
                                    android:text="Modificar Categorias"
                                    android:textColor="@color/colorAccent" />
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <View
                        android:id="@+id/divider4"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/com_facebook_button_background_color_disabled" />

                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@color/com_facebook_button_background_color_focused"
                android:orientation="vertical"
                android:paddingBottom="10dp">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp"
                    android:text="Comentarios"
                    android:textColor="@color/common_google_signin_btn_text_dark_focused"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ListView
                    android:id="@+id/ma_lv_lista"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="40dp"
                    android:background="@color/com_facebook_button_background_color_focused">
                    android:layout_marginTop="40dp" />
                </ListView>
            </RelativeLayout>
        </LinearLayout>

    </ScrollView>

</LinearLayout>